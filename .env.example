# Discord Bot Configuration
BOT_TOKEN=your_discord_bot_token_here

# Vinted Configuration
VINTED_BASE_URL=https://www.vinted.de/

# Proxy Configuration
PS_API_KEY=your_proxyscrape_api_key_here
SERVICE_ID=your_proxyscrape_service_id_here
PROXY_LIST_URL=https://api.proxyscrape.com/v2/account/datacenter_shared/proxy-list?auth=${PS_API_KEY}&type=getproxies&protocol=http&format=txt&status=all&country=all&service=${SERVICE_ID}

# Proxy Settings
ALLOW_DIRECT=0                    # Set to 1 to allow direct connections when no proxies available
PROXY_REFRESH_HOURS=6             # How often to refresh proxy pool (in hours)
MAX_PROXY_FAILS=5                 # Max failed proxy downloads before stopping refresh
LIST_REFRESH_MIN=180              # Minutes between proxy list refreshes
WHITELIST_SLEEP=300               # Seconds to wait after IP whitelist

# Environment
NODE_ENV=production               # Set to 'test' for testing mode